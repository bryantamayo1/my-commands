        "/users/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Login. Without token",
                "description": "Prevously, put {process.env.PATH_ADMIN}/api/v1 in the endpoint.",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "email",
                                "password"
                            ],
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "required": "true"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "userName": {
                                            "type": "string",
                                            "required": "true"
                                        },
                                        "email": {
                                            "type": "string",
                                            "required": "true"
                                        },
                                        "role": {
                                            "type": "string",
                                            "required": "true",
                                            "enum" : [
                                                "admin",
                                                "user"
                                            ]
                                        },
                                        "xen": {
                                            "type": "string",
                                            "required": "true"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/register": {
            "post": {
